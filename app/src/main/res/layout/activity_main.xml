<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <RelativeLayout
            android:layout_width="394dp"
            android:layout_height="723dp" tools:layout_editor_absoluteY="8dp" tools:layout_editor_absoluteX="9dp">

        <Button
                android:text="GO"
                android:layout_width="77dp"
                android:layout_height="wrap_content" android:id="@+id/gobtn"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="28dp"
                android:layout_marginEnd="28dp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="627dp"/>
        <WebView
                android:layout_width="368dp"
                android:layout_height="389dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="93dp" android:layout_marginTop="241dp"
                android:layout_alignParentTop="true" android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true" android:layout_marginRight="11dp"
                android:layout_marginEnd="11dp" android:id="@+id/mywebview"/>
        <EditText
                android:layout_width="248dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/url_address" android:layout_alignParentBottom="true" android:layout_marginBottom="630dp"
                android:layout_alignParentEnd="true" android:layout_alignParentRight="true"
                android:layout_marginRight="129dp" android:layout_marginEnd="129dp"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>